<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/feed_page_bg"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/title_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <Button
            android:layout_marginLeft="20dp"
            android:id="@+id/feed_ac_back_btn"
            android:layout_width="30dp"
            android:background="@drawable/superplayer_btn_back_play"
            android:layout_height="30dp"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/feed_ac_layout_txt"
            android:text="@string/app_feed_title"
            android:textColor="@color/superplayer_white"
            android:textSize="16sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true" />

    </RelativeLayout>

    <com.tencent.liteav.demo.player.demo.feed.FeedView
        android:background="@color/superplayer_white"
        android:id="@+id/feed_ac_feed_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>